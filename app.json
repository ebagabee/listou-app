{
  "expo": {
    "name": "listou-app",
    "slug": "listou-app",
    "version": "0.1.0",
    "scheme": "listou",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/loadscreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#FF730F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gajoy.listouapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/loadscreen.png",
        "backgroundColor": "#FF730F"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gajoy.listouapp"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": [
              "-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"
            ]
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a2c54e80-c3af-4dc2-8fab-3d416785f9b8"
      }
    }
  }
}
